{
  "private": true,
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.0",
    "gulp": "^3.9.1",
    "morphdom": "^2.3.3"
  },
  "scripts": {
    "start": "webpack-dev-server -d",
    "build:rollup": "rollup -c rollup.config.js",
    "p-postbuild": "uglifyjs build.js -m -c unsafe,collapse_vars,evaluate,screw_ie8,loops,keep_fargs=false,pure_getters,unused,dead_code --pure-funcs classCallCheck Object.freeze invariant warning -o build.js",
    "build": "browserify src/app.js --debug -o build.js",
    "dist": "browserify src/app.js | uglifyjs -m -c unsafe,collapse_vars,evaluate,screw_ie8,loops,keep_fargs=false,pure_getters,unused,dead_code --pure-funcs classCallCheck Object.freeze invariant warning > build.js",
    "test": "browserify test-setdom.js --debug -o build.js",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules --color --display minimal"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "babelify": "^8.0.0",
    "gulp": "^1.0.0",
    "gulp-shell": "^0.6.5",
    "gulp-watch": "^5.0.1",
    "handlebars": "^4.0.11",
    "handlebars-delimiters": "^1.0.0",
    "harmony-reflect": "^1.5.1",
    "html-loader": "^0.5.5",
    "html-template-tag": "^2.0.0",
    "html-webpack-plugin": "^2.30.1",
    "keet": "git+https://git@github.com/syarul/keet.git#morph",
    "nanohtml": "^1.2.4",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "todomvc-app-css": "^2.1.2",
    "todomvc-common": "^1.0.5",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "babel": {
    "presets": [
      [
        "env"
      ]
    ],
    "plugins": [
      "external-helpers",
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  }
}
